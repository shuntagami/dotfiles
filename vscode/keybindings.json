[
  {
    "key": "cmd+shift+r",
    "command": "workbench.action.reloadWindow",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd",
    "command": "rubyLsp.restart"
  },
  {
    "key": "cmd+i",
    "command": "composerMode.agent"
},
{
    "key": "shift+enter",
    "command": "workbench.action.terminal.sendSequence",
    "args": {
        "text": "\\\r\n"
    },
    "when": "terminalFocus"
}
]
